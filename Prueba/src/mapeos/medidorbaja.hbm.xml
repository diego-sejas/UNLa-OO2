<?xml version = "1.0" encoding = "UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name = "datos.MedidorBaja" table = "medidorbaja">
		<id column = "idMedidorBaja" name = "idMedidorBaja">
			<generator class = "identity"/>
		</id>
		<many-to-one name="cliente" class="datos.Cliente"
			column="idCliente" not-null="true" />
		<property column = "nroSerie" name = "nroSerie" type = "string"/>
		<property column = "domicilio" name = "domicilio" type = "string"/>
		
		
	</class>
</hibernate-mapping>